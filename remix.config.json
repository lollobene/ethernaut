{
  "solidity-compiler": {
    "language": "Solidity",
    "settings": {
      "optimizer": {
        "enabled": true,
        "runs": 200
      },
      "outputSelection": {
        "*": {
          "": [
            "ast"
          ],
          "*": [
            "abi",
            "metadata",
            "devdoc",
            "userdoc",
            "storageLayout",
            "evm.legacyAssembly",
            "evm.bytecode",
            "evm.deployedBytecode",
            "evm.methodIdentifiers",
            "evm.gasEstimates",
            "evm.assembly",
            "ir"
          ]
        },
        "MagicNumber": {
          "MagicNumber" : [
            "ir",
            "irAst",
            "irOptimized",
            "irOptimizedAst"
          ]
        }
      }
    }
  }
}